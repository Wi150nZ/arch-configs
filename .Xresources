URxvt*scrollBar: false
URxvt.font: xft: tamsyn:size=12

! special
*.foreground:   #efefef
*.background:   #21222c
*.cursorColor:  #efefef

! black
*.color0:       #21222c
*.color8:       #667cf3

! red
*.color1:       #ff7ac6
*.color9:       #ff7ac6

! green
*.color2:       #47f37b
*.color10:      #47f37b

! yellow
*.color3:       #f1fa8c
*.color11:      #f1fa8c

! blue
*.color4:       #a4acd9
*.color12:      #a4acd9

! magenta
*.color5:       #ff7ac6
*.color13:      #ff7ac6

! cyan
*.color6:       #22d1ff
*.color14:      #22d1ff

! white
*.color7:       #efefef
*.color15:      #667cf3

! ------------------------------------------------------------------------------
! ! ! ROFI Color theme ashes
! ! ! ------------------------------------------------------------------------------
! ! ! Use extended color scheme
! rofi.color-enabled:                  true
! ! Color scheme for normal row
! rofi.color-normal:                   #ffffff,#525d76,#f5f5f5,#95aec7,#f3f4f5
! ! Color scheme for urgent row
! rofi.color-urgent:                   #ffffff,#dc322f,#f5f5f5,#5294e2,#393f45
! ! Color scheme for active row
! rofi.color-active:                   #ffffff,#c2cad0,#f5f5f5,#5294e2,#393f45
! ! Color scheme window
! rofi.color-window:                   #ffffff,#f5f5f5
! ! Separator style (none, dash, solid)
! rofi.separator-style:                solid
!
Xft.antialias: 1
Xft.autohint: 0
Xft.dpi: 96
Xft.hinting: 1
Xft.hintstyle: hintslight
Xft.lcdfilter: lcddefault
Xft.rgba: rgb
